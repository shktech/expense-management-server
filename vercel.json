{
    "version": 2,
    "builds": [
      {
        "src": "vercel.py",
        "use": "@vercel/python"
      },
      {
        "src": "static/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "vercel.py"
      }
    ],
    "env": {
      "DJANGO_SETTINGS_MODULE": "Template.settings"
    }
  }